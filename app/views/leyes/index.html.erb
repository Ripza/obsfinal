<h1>Listing leyes</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Tema</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @leyes.each do |leye| %>
  <tr>
    <td><%= leye.nombre %></td>
    <td><%= leye.tema %></td>
    <td><%= link_to 'Show', leye %></td>
    <td><%= link_to 'Edit', edit_leye_path(leye) %></td>
    <td><%= link_to 'Destroy', leye, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

